{
   "name": "Ai Display",
   "version": "0.1.1",
   "description": "AI Display helps you quickly find accurate answers and display relevant questions, making information discovery faster and smarter. (Â© elsesourav 2025)",
   "manifest_version": 3,
   "permissions": [
      "scripting",
      "storage",
      "tabs",
      "activeTab",
      "management",
      "declarativeNetRequest",
      "declarativeNetRequestFeedback",
      "webRequest",
      "declarativeNetRequestWithHostAccess",
      "unlimitedStorage",
      "offscreen"
   ],
   "optional_permissions": ["clipboardWrite"],
   "host_permissions": ["<all_urls>"],
   "background": {
      "service_worker": "./background/background.js"
   },
   "homepage_url": "https://github.com/elsesourav",
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
   },
   "content_scripts": [
      {
         "matches": ["<all_urls>"],
         "js": ["./utils.js", "./content/content.js"]
      }
   ],
   "web_accessible_resources": [
      {
         "resources": [
            "inject/inject.html",
            "inject/Tesseract/*",
            "inject/Lang/*"
         ],
         "matches": ["<all_urls>"]
      }
   ],
   "action": {
      "default_icon": {
         "16": "assets/icons/16.png",
         "24": "assets/icons/24.png",
         "48": "assets/icons/48.png",
         "32": "assets/icons/32.png",
         "128": "assets/icons/128.png"
      },
      "default_title": "Ai Display",
      "default_popup": "./popup.html"
   },
   "icons": {
      "16": "assets/icons/16.png",
      "24": "assets/icons/24.png",
      "48": "assets/icons/48.png",
      "32": "assets/icons/32.png",
      "128": "assets/icons/128.png"
   }
}
